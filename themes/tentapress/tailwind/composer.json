{
    "name": "tentaplane/theme-tailwind",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "tentapress/themes": "<1.0"
    },
    "autoload": {
        "psr-4": {
            "TentaPress\\Themes\\Tailwind\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TentaPress\\Themes\\Tailwind\\TailwindThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
