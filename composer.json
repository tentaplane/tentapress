{
    "name": "tentaplane/tentapress",
    "version": "0.15.5",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "tentapress/admin-shell": "<1.0",
        "tentapress/blocks": "<1.0",
        "tentapress/media": "<1.0",
        "tentapress/menus": "<1.0",
        "tentapress/pages": "<1.0",
        "tentapress/posts": "<1.0",
        "tentapress/seo": "<1.0",
        "tentapress/settings": "<1.0",
        "tentapress/system": "<1.0",
        "tentapress/system-info": "<1.0",
        "tentapress/themes": "<1.0",
        "tentapress/users": "<1.0"
    },
    "suggest": {
        "tentapress/import": "Import tooling",
        "tentapress/export": "Export tooling",
        "tentapress/static-deploy": "Static deployments",
        "tentapress/block-markdown-editor": "Markdown based rich text editor block",
        "tentapress/page-editor": "Notion-style page editor for TentaPress"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/*/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm --prefix plugins/tentapress/admin-shell run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan tp:plugins sync --no-interaction"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
